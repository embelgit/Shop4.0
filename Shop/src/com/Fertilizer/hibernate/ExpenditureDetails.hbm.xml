<?xml version="1.0"?>
 <!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" > 

    <hibernate-mapping>
    
    <class name="com.Fertilizer.hibernate.ExpenditureDetailsBean" 
    table="expenditure_details" schema="shop"
    >
    
    	<id name="pkExpenseDetailsId" type="java.lang.Long" column="pk_expense_details_id">
			<generator class="increment" />
		</id>


		<property 
		name="expenseName" 
		type="java.lang.String" 
		column="expense_name"
		length="128" />
		
		<property
        name="insertDate"
        type="java.sql.Date"
        column="insert_date"  />
        
        <property 
		name="decription" 
		type="java.lang.String" 
		column="description"
		length="128" />
		
		<property 
		name="fkshopid" 
		type="java.lang.Long" 
		column="fk_shop_id"
		length="128" />
		
		<property 
		name="fkuserid" 
		type="java.lang.Long" 
		column="fk_user_id"
		length="128" />
		
		<property 
		name="shopName" 
		type="java.lang.String" 
		column="shop_name"
		length="128" />
						
		<property 
		name="userName" 
		type="java.lang.String" 
		column="user_name"
		length="128" />
		
		<property 
		name="expenseType" 
		type="java.lang.String" 
		column="expense_type"
		length="128" />
			
		</class>
    </hibernate-mapping>